{
  "name": "ws-flare-test-client",
  "version": "1.0.0",
  "description": "ws-flare-test-client",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "cross-env USER_API=http://user.com PROJECTS_API=http://projects.com JOBS_API=http://jobs.com KUBERNETES_SERVICE_HOST=localhost KUBERNETES_SERVICE_PORT=9000 JOB_ID=job1 URI=ws://localhost:9001 SCRIPT_INDEX=1 NODE_ID=node1 TOTAL_SIMULATORS=1000 lb-mocha -t 60000 --allow-console-logs \"dist/__tests__\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t ws-flare-graphql .",
    "docker:run": "docker run -p 3000:3000 -d ws-flare-graphql",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "cross-env JWT_SECRET=test node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.1.0",
    "@loopback/context": "^1.7.0",
    "@loopback/core": "^1.1.8",
    "@loopback/openapi-v3": "^1.3.0",
    "@loopback/repository": "^1.1.8",
    "@loopback/rest": "^1.8.0",
    "@loopback/rest-explorer": "^1.1.11",
    "@loopback/service-proxy": "^1.0.10",
    "amqplib": "^0.5.3",
    "express": "^4.16.4",
    "kubernetes-client": "^6.11.0",
    "lodash": "^4.17.11",
    "superagent": "^4.1.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "ws": "^6.2.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.3.2",
    "@loopback/testlab": "^1.1.0",
    "@loopback/tslint-config": "^2.0.2",
    "@types/amqplib": "^0.5.11",
    "@types/async": "^2.4.1",
    "@types/chai": "^4.1.7",
    "@types/express-jwt": "^0.0.41",
    "@types/graphql": "^14.0.7",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mock-fs": "^3.6.30",
    "@types/nock": "^9.3.1",
    "@types/node": "^10.11.2",
    "@types/superagent": "^4.1.1",
    "@types/uuid": "^3.4.4",
    "@types/ws": "^6.0.1",
    "async": "^2.6.2",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "isomorphic-fetch": "^2.2.1",
    "mock-fs": "^4.8.0",
    "nock": "^10.0.6",
    "node-docker-api": "^1.1.22",
    "random-port-as-promised": "^1.0.0",
    "tslint": "^5.12.0",
    "typescript": "^3.3.1"
  }
}
